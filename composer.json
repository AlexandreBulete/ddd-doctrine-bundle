{
    "name": "alexandrebulete/ddd-doctrine-bundle",
    "description": "Symfony Bundle for DDD Doctrine Bridge - Service wiring and configuration",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre Bulete"
        }
    ],
    "require": {
        "php": "^8.2",
        "alexandrebulete/ddd-doctrine-bridge": "^1.0",
        "symfony/framework-bundle": "^7.0",
        "symfony/config": "^7.0",
        "symfony/dependency-injection": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Alexandrebulete\\DddDoctrineBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alexandrebulete\\DddDoctrineBundle\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}

